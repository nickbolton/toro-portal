<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition PUBLIC "-//uPortal//Channel Definition/EN" "channelDefinition.dtd">

<!-- You can download this Google Sample Portlet from the Portlet Open Source Trading site (POST).
     http://portlet-opensrc.sourceforge.net/
     http://prdownloads.sourceforge.net/portlet-opensrc/GooglePortlet.zip?download
-->

<channel-definition>
    <title>Toro Briefcase Portlet</title>
    <name>Toro Briefcase Portlet</name>
    <fname>toro-briefcase-portlet</fname>
    <desc>Toro Briefcase Portlet</desc>
    <type>Portlet</type>
    <class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
    <timeout>60000</timeout>

    <hasedit>N</hasedit>
    <hashelp>N</hashelp>
    <hasabout>N</hasabout>

    <secure>N</secure>
    <locale>en_US</locale>

    <categories>
        <category>Applications</category>
    </categories>

    <groups>
        <group>Everyone</group>
    </groups>

    <parameters>
        <parameter>
            <name>portletDefinitionId</name>
            <value>toro-briefcase-portlet.BriefcasePortlet</value>
            <description>Identifies the portlet deployed within the portlet container</description>
            <ovrd>N</ovrd>
        </parameter>
    </parameters>
</channel-definition>
