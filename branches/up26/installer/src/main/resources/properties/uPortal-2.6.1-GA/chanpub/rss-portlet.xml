<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition PUBLIC "-//uPortal//Channel Definition/EN" "channelDefinition.dtd">

<!-- You can download this RSS Sample Portlet from the Portlet Open Source Trading site (POST).
     http://portlet-opensrc.sourceforge.net/
     http://prdownloads.sourceforge.net/portlet-opensrc/RssPortlet.zip?download
-->

<channel-definition>

    <title>RSS Portlet</title>
    <name>RSS Portlet</name>
    <fname>rss-portlet</fname>
    <desc>RSS Portlet from Portlet Open Source Trading site</desc>
    <type>Portlet</type>
    <class>org.jasig.portal.channels.portlet.CPortletAdapter</class>
    <timeout>60000</timeout>

    <hasedit>Y</hasedit>
    <hashelp>Y</hashelp>
    <hasabout>N</hasabout>

    <secure>N</secure>
    <locale>en_US</locale>

    <categories>
        <category>Development</category>
    </categories>

    <groups>
        <group>Everyone</group>
    </groups>

    <parameters>

        <!-- The syntax of the ID is [portlet-context-name].[portlet-name] -->
        <parameter>
            <name>portletDefinitionId</name>
            <value>RssPortlet.RssPortlet</value>
            <description>Identifies the portlet deployed within the portlet container</description>
            <ovrd>N</ovrd>
        </parameter>

    </parameters>

</channel-definition>
