  <page page-id="2" title="Graduate Senators">
    <question question-id="1">Please Vote for TWO or fewer candidates.
      <input input-id="1">
        <data type="Check" form="Column">
          <entry data-id="1" href="http://www.ilstu.edu/elections/index.htm#Amy_Attivissimo">Amy Attivissimo (Psychology)</entry>
          <entry data-id="2" href="http://www.ilstu.edu/elections/index.htm#David_R_Forest">David R. Forest (Technology)</entry>
        </data>
      </input>
      <input input-id="2">Other:
        <data type="Text" form="Single"/>
      </input>
      <input input-id="3">Other:
        <data type="Text" form="Single"/>
      </input>
    </question>
  </page>
  <page page-id="3" title="Student Trustee">
    <question question-id="1">Student Trustee
      <input input-id="1">
        <data type="Choice" form="Column">
          <entry data-id="1" href="http://www.ilstu.edu/elections/index.htm#Adam_W_Christ">Adam W. Christ (Political Science) </entry>
          <entry data-id="2">James Klepk (Political Science)</entry>
          <entry data-id="3">Other:</entry>
        </data>
