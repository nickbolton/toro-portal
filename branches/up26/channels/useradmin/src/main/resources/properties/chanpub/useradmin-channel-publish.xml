<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE channel-definition PUBLIC "-//uPortal//Channel Definition/EN" "channelDefinition.dtd">

<!-- You can download this Google Sample Portlet from the Portlet Open Source Trading site (POST).
     http://portlet-opensrc.sourceforge.net/
     http://prdownloads.sourceforge.net/portlet-opensrc/GooglePortlet.zip?download
-->

<channel-definition>
    <title>Toro User Admin Channel</title>
    <name>Toro User Admin Channel</name>
    <fname>toro-useradmin-channel</fname>
    <desc>Toro User Admin Channel</desc>
    <type>Custom</type>
    <class>net.unicon.portal.channels.PortletMultithreadedChannel</class>
    <timeout>60000</timeout>

    <hasedit>N</hasedit>
    <hashelp>Y</hashelp>
    <hasabout>N</hasabout>

    <secure>N</secure>
    <locale>en_US</locale>

    <categories>
        <category>Applications</category>
    </categories>

    <groups>
        <group>Everyone</group>
    </groups>

    <parameters>
        <parameter>
            <name>cscrEnabled</name>
            <value>true</value>
            <description>Enables Client-Side Channel Rendering</description>
            <ovrd>N</ovrd>
        </parameter>
        <parameter>
            <name>producer</name>
            <value>net.unicon.academus.apps.useradmin.UserAdminProducer</value>
            <description>null</description>
            <ovrd>N</ovrd>
        </parameter>
        <parameter>
            <name>permissions</name>
            <value>permissions.xml</value>
            <description>null</description>
            <ovrd>N</ovrd>
        </parameter>
        <parameter>
            <name>baseHelpUrl</name>
            <value>help_Portal_User_Administration_Channel.html</value>
            <description>null</description>
            <ovrd>N</ovrd>
        </parameter>
        <parameter>
            <name>type</name>
            <value>Admin</value>
            <description>null</description>
            <ovrd>N</ovrd>
        </parameter>
        <parameter>
            <name>sslLocation</name>
            <value>useradmin/UserAdminChannel.ssl</value>
            <description>null</description>
            <ovrd>N</ovrd>
        </parameter>
    </parameters>
</channel-definition>
